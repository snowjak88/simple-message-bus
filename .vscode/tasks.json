{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build (Debug)",
			"command": "dotnet",
			"type": "process",
			"args": ["build", "${workspaceFolder}/SimpleMessageBus.csproj"],
			"problemMatcher": "$msCompile",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Build (Release)",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/SimpleMessageBus.csproj",
				"-c",
				"Release"
			],
			"problemMatcher": "$msCompile"
		}
	]
}
